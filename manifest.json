{
    "manifest_version": 3,
    "name": "Swagger editor support",
    "version": "1.0",
    "description": "A Swagger editor supporter that organize your api design",
    "action": {
        "default_popup": "/popup/popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [
        "activeTab"
    ]
}